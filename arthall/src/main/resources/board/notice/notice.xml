<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="notice">

	<select id="count" parameterType="notice.NoticeVO" resultType="Integer">

		SELECT COUNT(*) FROM notice
	
	</select>


	<select id="list" parameterType="notice.NoticeVO" resultType="notice.NoticeVO">
		
		SELECT * FROM notice 
	 
	</select>
</mapper>

